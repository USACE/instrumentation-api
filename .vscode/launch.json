  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/root/main.go",
      "env": {
        "INSTRUMENTATION_LAMBDACONTEXT": "false",
        "INSTRUMENTATION_SKIPJWT": "true",
        "INSTRUMENTATION_DBUSER": "postgres",
        "INSTRUMENTATION_DBPASS": "postgres",
        "INSTRUMENTATION_DBNAME": "postgres",
        "INSTRUMENTATION_DBHOST": "localhost",
        "INSTRUMENTATION_DBSSLMODE": "disable"
      },
      "args": []
    }
  ]
}
